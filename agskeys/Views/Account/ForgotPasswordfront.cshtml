@{
    Layout = null;
}

<div class="modal-body" style="min-height:200px;">
    <div class="row align-items-end">
        <div class="col-lg-8">

        </div>
        <div class="col-lg-4">
            <button type="button" class="close" style="font-size:30px" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
    </div>
    <section class="login-block" style="padding:0px;">
        <!-- Container-fluid starts -->
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-12" >
                    <!-- Authentication card start -->
                    <form class="form-horizontal f1" id="login-form" method="post" action="@Url.Content("/Account/ForgotPasswordfront/")">
                        <div class="col-md-12">
                            <h3 class="text-center txt-primary">Forgot Your Password.?</h3>
                        </div>
                        <div class="auth-box card">
                            <div class="card-block">
                                <div class="row m-b-20">
                                    
                                </div>
                                @if (TempData["NotExst"] != null)
                                {

                                    <div class="alert alert-danger alert-dismissible" role="alert">
                                        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                        @TempData["NotExst"].ToString()
                                    </div>
                                    <script>

                                        alert("@TempData["NotExst"].ToString()");
                                    </script>

                                }
                                @if (TempData["mail"] != null)
                                {

                                    <div class="alert alert-success alert-dismissible" role="alert">
                                        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                        @TempData["mail"].ToString()
                                    </div>
                                    <script>

                                        alert("@TempData["mail"].ToString()");
                                    </script>

                                }
                                @if (TempData["email"] != null)
                                {

                                    <div class="alert alert-danger alert-dismissible" role="alert">
                                        <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                        @TempData["email"].ToString()
                                    </div>
                                    <script>

                                        alert("@TempData["email"].ToString()");
                                    </script>

                                }
                                <div class="form-group form-primary col-md-12" style="padding: 20px 20px 0px 20px;">
                                    <label class="float-label">Select Your Userlevel</label>
                                    <select class="ddl-bg form-control" id="userlevel" name="userlevel" required="required">
                                        <option class="opt-1" value="">User type</option>
                                        <option class="opt-1" value="partner">Partner</option>
                                        <option class="opt-1" value="clientele">Customer</option>
                                        <option class="opt-1" value="sales_executive">Sales Executive</option>
                                    </select>

                                </div>
                                
                                <div class="form-group form-primary col-md-12" style="padding: 20px 20px 0px 20px;">
                                    <label class="float-label">Enter Your Username Here</label>
                                    <input type="text" id="userName" name="userName" class="form-control" placeholder="user name" required="required" autofocus="autofocus">
                                    <span class="form-bar"></span>

                                </div>
                                
                                <div class="row m-t-15">
                                    <div class="col-md-2">
                                        
                                    </div>
                                    <div class="col-md-8">
                                        <button type="submit" id="reset" value="reset" name="reset" class="btn btn-primary btn-md btn-block waves-effect text-center m-b-20" style="background-color: #d91f27; border-color: white;margin-top: 15px;">RESET PASSWORD</button>

                                    </div>
                                    <div class="col-md-2">
                                       
                                    </div>
                                </div>

                            </div>
                            <p style="text-align:center; margin-left:10px; margin-right:10px;"><i class="far fa-copyright"></i><span style="color:#808080"> 2019 - 2020</span> Softmagic.in. <span style="color:#808080">All Rights Reserved</span> </p>
                        </div>
                    </form>
                </div>

                @*<div class="col-sm-6">

                    </div>*@
                <!-- Authentication card end -->
                <div style="text-align:center">

                </div>
            </div>
            <!-- end of col-sm-12 -->
        </div>
        <!-- end of row -->
        <!-- end of container-fluid -->
    </section>
    </div>

