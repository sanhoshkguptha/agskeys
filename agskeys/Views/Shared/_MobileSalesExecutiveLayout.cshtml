</head>
<body>
    <div id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" style="display:block;" onclick="closeNav()"><i class="fa fa-angle-left" style="font-size:22px; padding-right: 10px;"></i> back</a>
        @*<a class="@(ViewContext.RouteData.Values["Action"].ToString() == "Customer" ? "active" : "")" href="~/MobileSalesExecutive/Customer"><i class="fa fa-bank"></i><span>CUSTOMER</span></a>*@
        <a class="@(ViewContext.RouteData.Values["Action"].ToString() == "salesloan" ? "active" : "")" href="~/MobileSalesLoan/salesloan"><i class="fa fa-bank"></i><span>LOAN</span></a>
        <a class="@(ViewContext.RouteData.Values["Action"].ToString() == "Support" ? "active" : "")" href="~/MobileSalesExecutive/Support"><i class="fa fa-globe"></i><span>SUPPORT</span></a>
    </div>

    @RenderBody()

    <footer>

        <div class="row disp-name11" id="sec1">


            <a href="~/MobileSalesLoan/salesloan" class="col-sm-4 col-xs-4 @(ViewContext.RouteData.Values["Action"].ToString() == "salesloan" ? "active" : "")"><i class="fa fa-home"></i><br> HOME </a>

            <a href="~/MobileSalesExecutive/UserProfile" class="col-sm-4 col-xs-4 @(ViewContext.RouteData.Values["Action"].ToString() == "UserProfile" ? "active" : "")"><i class="fa fa-user"></i> <br> PROFILE </a>

            <a href="#" data-toggle="modal" data-target="#myModal" class="col-sm-4 col-xs-4 "><i class="fa fa-sign-out"></i> <br>LOGOUT </a>

        </div>

    </footer>


    <script>

        function openNav() {
            document.getElementById("mySidenav").style.width = "40%";

        }

        function closeNav() {
            document.getElementById("mySidenav").style.width = "0";

        }
        var box = document.getElementById("L-body")
        var box1 = document.getElementById("L1-body")
        box.onclick = function () {

            document.getElementById("mySidenav").style.width = "0";
        }
        box1.onclick = function () {

            document.getElementById("mySidenav").style.width = "0";
        }
    </script>
    <script>
        function goBack() {
            window.history.back();
        }
    </script>

    <!-- jQuery -->
    <script src="~/ags-mobile/vendors/jquery/dist/jquery.min.js"></script>
    <!-- Bootstrap -->
    <script src="~/ags-mobile/vendors/bootstrap/dist/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/files/bower_components/jquery-ui/js/jquery-ui.min.js"></script>
  

    <!-- Custom Theme Scripts -->
    <script src="~/ags-mobile/build/js/custom.min.js"></script>
