@model IEnumerable<agskeys.Models.loan_table>
@{

    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_ClienteleLayout.cshtml";
}
<style>
    h3.head {
        color: white;
        text-shadow: 1px 1px 2px #5b5b5b;
    }
    
    </style>
<!-- [ breadcrumb ] start -->
<div class="page-header card">
    <div class="row align-items-end">
        <div class="col-lg-8">
            <div class="page-header-title">
                <i class="feather icon-home bg-c-blue"></i>
                <div class="d-inline">
                    <h5>Dashboard Clientele</h5>
                    <span>Aggressive Sales and Services</span>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            <div class="page-header-breadcrumb">
                <ul class=" breadcrumb breadcrumb-title">
                    <li class="breadcrumb-item">
                        <a href="#"><i class="feather icon-home"></i></a>
                    </li>
                    <li class="breadcrumb-item"><a href="#!">Dashboard Clientele</a> </li>
                </ul>
            </div>
        </div>
    </div>
</div>
<!-- [ breadcrumb ] end -->
<div class="pcoded-inner-content">
    <div class="main-body">
        <div class="page-wrapper">
            <div class="page-body">
                <!-- [ page content ] start -->
                <div class="row">
                    <div class="col-xl-4 col-md-4">
                        <div class="card comp-card">
                            <div class="card-body">
                                <div class="row align-items-center">
                                    <div class="col">
                                        <h6 class="m-b-25">Total Loans</h6>
                                        <h3 class="f-w-700 text-c-blue">@Model.Count()</h3>
                                        <p class="m-b-0">@DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt")</p>
                                    </div>
                                    <div class="col-auto">
                                        <i class="fa fa-eye bg-c-blue"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-4">
                        <div class="card comp-card">
                            <div class="card-body">
                                <div class="row align-items-center">
                                    <div class="col">
                                        <h6 class="m-b-25">Santioned Amount</h6>
                                        <h3 class="f-w-700 text-c-green">@ViewData["loanamnt"]</h3>
                                        <p class="m-b-0">@DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt")</p>
                                    </div>
                                    <div class="col-auto">
                                        <i class="fa fa-university bg-c-green"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-4 col-md-4">
                        <div class="card comp-card">
                            <div class="card-body">
                                <div class="row align-items-center">
                                    <div class="col">
                                        <h6 class="m-b-25">Avg Rate Of Interest</h6>
                                        <h3 class="f-w-700 text-c-yellow">@ViewData["interest"]%</h3>
                                        <p class="m-b-0">@DateTime.Now.ToString("dd/MM/yyyy hh:mm:ss tt")</p>
                                    </div>
                                    <div class="col-auto">
                                        <i class="fa fa-bank bg-c-yellow"></i>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                    <!-- sale revenue card end -->
                    <!-- Project statustic start -->
                <div class="row">
                    <div class="col-xl-12">
                        <div class="card proj-progress-card">
                            <div class="card-block">
                                <div class="row" >
                                    <div class="col-md-4" style="text-align:center"><img src="~/additionalimages/membership.png"/></div>
                                    <div class="col-md-8">
                                        <h3 style="color:#d92027" class="head">LAUCHED NEW MEMBERSHIP CARDS FOR CUSTOMERS</h3>
                                        <h5 style="color:#0a55ab">Existing / New Customer will get three types of Membership Card from AGSKEYS</h5>
                                        <table>
                                            <tr class="col">
                                                <td style="color:#d27f07">TYPE A - GOLD Membership Card - Reference up to 10 friends</td>
                                            </tr>
                                            <tr>
                                                <td style="color:#d27f07">TYPE B - PLATINUM Membership Card - Reference 11 - 20 friends</td>
                                            </tr>
                                            <tr>
                                                <td style="color:#d27f07">TYPE C - SIGNATURE Membership Card - Reference above 21 friends</td>
                                            </tr>
                                            <tr>
                                                <td style="color:#d92027">By reference of one loan Customer to AGSKEYS the Memeber and friend will earn 2000 points Each</td>
                                            </tr>
                                            <tr>
                                                <td style="color:#0c1c93">1.Gold Card Member 1 point is equal to 1/-</td>
                                            </tr>
                                            <tr>
                                                <td style="color:#0c1c93">2.Platinum Card Member 1 point is equal to 2/-</td>
                                            </tr>
                                            <tr>
                                                <td style="color:#0c1c93">3.Signature Card Member 1 point is equal to 3/-</td>
                                            </tr>
                                            <tr><td style="color:#034a0e">*** Conditions Applied</td></tr>
                                        </table>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col-xl-12">
                        <div class="card proj-progress-card">
                            <div class="card-block">
                                <div class="row">
                                    <div class="col-xl-3 col-md-6">
                                        <h6>Requeted Loan Amount</h6>
                                        @*@{
                                        int requestloan = @Model.Sum(j => (Convert.ToInt64(j.requestloanamt)));
                                        }*@
                                        <h5 class="m-b-30 f-w-700">@ViewData["requestamnt"]<span class="text-c-green m-l-10">100%</span></h5>
                                        <div class="progress">
                                            <div class="progress-bar bg-c-red" style="width:100%"></div>
                                        </div>
                                    </div>
                                    <div class="col-xl-3 col-md-6">
                                        <h6>Santioned Amount</h6>
                                        <h5 class="m-b-30 f-w-700">@ViewData["loanamnt"] <span class="text-c-red m-l-10">@ViewData["sanction_percentage"]%</span></h5>
                                        <div class="progress">
                                            <div class="progress-bar bg-c-blue" style="width:@ViewData["sanction_percentage"]%"></div>
                                        </div>
                                    </div>
                                    <div class="col-xl-3 col-md-6">
                                        <h6>Disbursed Amount</h6>
                                        <h5 class="m-b-30 f-w-700">@ViewData["disbursementamnt"]<span class="text-c-green m-l-10">@ViewData["disbursement_percentage"]%</span></h5>
                                        <div class="progress">
                                            <div class="progress-bar bg-c-green" style="width:@ViewData["disbursement_percentage"]%"></div>
                                        </div>
                                    </div>
                                    <div class="col-xl-3 col-md-6">
                                        <h6>Balance Amount</h6>
                                        <h5 class="m-b-30 f-w-700">@ViewData["balance"]<span class="text-c-green m-l-10">@ViewData["balance_percentage"]%</span></h5>
                                        <div class="progress">
                                            <div class="progress-bar bg-c-yellow" style="width:@ViewData["balance_percentage"]%"></div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-md-12">
                        <div class="card table-card">
                            <div class="card-header">
                                <h5>Loan Details</h5>
                                <div class="card-header-right">
                                    <ul class="list-unstyled card-option">
                                        <li class="first-opt"><i class="feather icon-chevron-left open-card-option"></i></li>
                                        <li><i class="feather icon-maximize full-card"></i></li>
                                        <li><i class="feather icon-minus minimize-card"></i></li>
                                        <li><i class="feather icon-refresh-cw reload-card"></i></li>
                                        <li><i class="feather icon-trash close-card"></i></li>
                                        <li><i class="feather icon-chevron-left open-card-option"></i></li>
                                    </ul>
                                </div>
                            </div>
                            <div class="col-md-12">
                                <div class="card-block p-b-0">
                                    <div class="table-responsive">
                                        <table id="alt-pg-dt" class="table table-striped table-bordered nowrap">
                                            <thead>
                                                <tr>
                                                    <th>#</th>
                                                    <th>
                                                        Bank
                                                    </th>
                                                    <th>
                                                        Loan Amount
                                                    </th>
                                                    <th>
                                                        Interset
                                                    </th>
                                                    <th>
                                                        Disbursement Amount
                                                    </th>
                                                   
                                                    <th>
                                                        Status
                                                    </th>
                                                    <th>Actions</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @{
                                                int i = 0;
                                                }
                                                @foreach (var item in Model)
                                                {
                                                i++;
                                                <tr>
                                                    <td>@i</td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.bankid)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.loanamt)
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.rateofinterest)%
                                                    </td>
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.disbursementamt)
                                                    </td>
                                                   
                                                    <td>
                                                        @Html.DisplayFor(modelItem => item.loanstatus)
                                                    </td>
                                                    <td>


                                                        <a href="#" id="Track" onclick="createModal('@Url.Action("Track", "Clientele" , new {id=item.id })')"><i class="icon feather icon-map-pin f-w-600 f-16 m-r-15" style="color:#fb8d00;"></i></a>


                                                        <a href="#" id="details" onclick="createModal('@Url.Action("Details", "Clientele" , new {id=item.id })')"><i class="icon feather icon-info f-w-600 f-16 m-r-15" style="color:#0042fb;"></i></a>



                                                        @*@foreach (var loan in ViewBag.assigned_loan)
                                                        {
                                                        if (loan.loanid == item.id.ToString())
                                                        {

                                                        <a href="#" id="edit" onclick="createModal('@Url.Action("Edit", "ProcessLoan" , new {id=item.id })')"><i class="icon feather icon-edit f-w-600 f-16 m-r-15 text-c-green"></i></a>
                                                        }
                                                        }*@
                                                    </td>
                                                </tr>
                                                }
                                            </tbody>
                                            <tfoot>
                                                <tr>

                                                    <th>#</th>
                                                    <th>
                                                        Bank
                                                    </th>
                                                    <th>
                                                        Loan Amount
                                                    </th>
                                                    <th>
                                                        Interset
                                                    </th>
                                                    <th>
                                                        Disbursement Amount
                                                    </th>
                                                   
                                                    <th>
                                                        Status
                                                    </th>
                                                    <th>Actions</th>
                                                </tr>
                                            </tfoot>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-xl-6 col-md-12">

                        <div class="card">
                            <div class="card-header">
                                <h5>Send Enquiry</h5>
                               
                                <span>Add class of <code>.form-control</code> with <code>&lt;input&gt;</code> tag</span>
                            </div>
                            
                            <div class="card-block">
                                <div>
                                    @if (TempData["customerSuccessMsg"] != null)
                                    {

                                        <div class="alert alert-success alert-dismissible" role="alert">
                                            <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                            @TempData["customerSuccessMsg"].ToString()
                                        </div>

                                    }
                                    @if (TempData["customerFailedMsg"] != null)
                                    {

                                        <div class="alert alert-danger alert-dismissible" role="alert">
                                            <button type="button" class="close" data-dismiss="alert"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                                            @TempData["customerFailedMsg"].ToString()
                                        </div>

                                    }
                                </div>
                                <form id="main" method="post" action="@Url.Content("/Clientele/SendEnquiry/")" >
                                    @foreach (var custprf in ViewBag.customer_profile_table)
                                    {
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Name</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" name="customerName" id="customerName" placeholder="Text Input Validation" value="@custprf.name">
                                            <span class="messages"></span>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Email</label>
                                        <div class="col-sm-10">
                                            <input type="email" class="form-control" id="customerEmail" name="customerEmail" placeholder="Enter valid e-mail address" value="@custprf.email">
                                            <span class="messages"></span>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Mobile</label>
                                        <div class="col-sm-10">
                                            <input type="text" class="form-control" name="customerPhone" id="customerPhone" placeholder="Mobile Numbers Only" value="@custprf.phoneno">
                                            <span class="messages"></span>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-sm-2 col-form-label">Message</label>
                                        <div class="col-sm-10">
                                            <textarea class="form-control" id="customerMessage" name="customerMessage" placeholder="Message"></textarea>
                                            <span class="messages"></span>
                                        </div>
                                    </div>

                                    <div class="form-group row">
                                        <label class="col-sm-2"></label>
                                        <div class="col-sm-10">
                                            <button type="submit" class="btn btn-primary m-b-0">Submit</button>
                                        </div>
                                    </div>
                                    }
                                </form>
                            </div>
                        </div>
                        <!-- Basic Inputs Validation end -->
                    </div>
                </div>
                    <!-- testimonial and top selling end -->
                    <!-- lattest update, new client end -->
                </div>
                <!-- [ page content ] end -->
            </div>
        </div>
        <div id="styleSelector">
        </div>
    </div>
</div>



